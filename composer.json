{
    "name": "mmucklo/inflections",
    "type": "library",
    "description": "A set of inflections for use with inflectors",
    "license": "MIT",
    "keywords": ["singularize", "pluralize", "inflect", "inflector"],
    "authors": [
	{
	    "name": "Matthew J. Mucklo",
	    "email": "mmucklo@gmail.com"
	}
    ],
    "require": {
        "php": ">=5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.0",
        "friendsofphp/php-cs-fixer": "dev-master",
        "phpunit/php-code-coverage": "^4.0"
    },
    "autoload": {
        "psr-4": {
	    "Inflections\\": "src/"
	}
    },
    "autoload-dev": {
        "psr-4": { "Inflections\\Tests\\": "test/" }
    },
    "config": {
        "bin-dir": "bin"
    }
}
